# platform :ios, '9.0'

target 'Tinder Firestore' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Tinder Firestore
pod 'Firebase/Firestore’
pod 'Firebase/Storage’
pod 'Firebase/Auth’
pod 'Firebase/Core’
pod 'SDWebImage'
pod 'JGProgressHUD'
pod 'LBTATools'
end
